{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "rules": {
    "interface-over-type-literal": false,
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "no-console": false,
    "jsx-no-lambda": false,
    "no-shadowed-variable": false,
    "jsx-boolean-value": false,
    "array-type": false
  },
  "jsRules": {
    "no-console": false
  },
  "linterOptions": {
    "exclude": [
      "**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  }
}
